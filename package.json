{
  "name": "filely-ssh-sftp-client",
  "productName": "Filely App",
  "version": "0.0.1-alpha",
  "description": "A clean and modern ssh and sftp client for windows, macos and linux",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/macOS.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . filely --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/windows.ico --prune=true --out=release-builds --version-string.CompanyName=Filely --version-string.FileDescription=Filely --version-string.ProductName=\"Filely Client SSH and SFTP\"",
    "package-linux": "electron-packager . filely --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/icons/linux.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/AlexanderMandera/Filely",
  "keywords": [
    "Electron",
    "ssh",
    "sftp",
    "client",
    "windows",
    "linux",
    "modern",
    "clean"
  ],
  "author": "Alexander Mandera, Robin Schleser & ilou",
  "license": "LICENSE.md",
  "devDependencies": {
    "electron": "^6.0.1",
    "electron-packager": "^14.0.4",
    "ssh2": "^0.8.5",
    "tsc": "^1.20150623.0"
  },
  "dependencies": {
    "discord-rpc": "^3.0.2",
    "discord.js": "^11.5.1",
    "electron": "^6.0.1",
    "electron-localshortcut": "^3.1.0",
    "electron-packager": "^14.0.4",
    "express": "^4.17.1",
    "node-png": "^0.4.3",
    "rfb2": "^0.2.0",
    "ssh2": "^0.8.5"
  }
}
